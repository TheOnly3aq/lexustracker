{
  "ignorePatterns": ["**/*.d.ts"],
  "plugins": ["unused-imports"],
  "extends": ["prettier"],
  "rules": {
    "no-unused-vars": "off",
    "unused-imports/no-unused-imports": "error",
    "react-hooks/exhaustive-deps": "off",
    "testing-library/prefer-screen-queries": "off",
    "@typescript-eslint/no-unused-vars": "off"
  }
}
